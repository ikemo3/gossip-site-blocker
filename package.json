        "html-webpack-plugin": "^5.5.0",
        "jest": "^28.1.3",
        "jest-environment-jsdom": "^29.3.1",
        "prettier": "^2.7.1",
        "ts-jest": "^28.0.8",
        "ts-loader": "^9.4.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3",
